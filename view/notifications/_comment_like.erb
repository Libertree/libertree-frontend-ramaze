<% if @like %>
  <%= render_file 'view/notifications/_notification_head.erb', notifications: @notifications, avatar_member: @like.member %>
    <% link = "<a href=\"#{ comment_link(@like.comment) }\" title=\"#{::CGI.escape_html(@like.comment.glimpse)}\">" %>
    <% if @like.comment.post.member == account.member %>
      <%=s_("#{@sg_or_pl}|%1$s liked your %2$scomment%3$s on your post.") % [@actors, link, "</a>"]%>
    <% else %>
      <%=s_("#{@sg_or_pl}|%1$s liked your %2$scomment%3$s on %4$s's post.") % [@actors, link, "</a>", @like.comment.post.member.name_display]%>
    <% end %>
  <%= render_file 'view/notifications/_notification_foot.erb', notifications: @notifications, glimpse: @like.comment.glimpse %>
<% end %>
